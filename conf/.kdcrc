{
  "plugins": {
    "kuzzle-enterprise-probe": {
      "storageIndex": "probes_iot",
      "probes": {
        "probe_watcher_1": {
          "type":"watcher",
          "index": "iot",
          "collection": "device-state",
          "action": "create",
          "collects": [
            "state.temperature",
            "state.light_level",
            "state.relative_humidity"
          ]
        }
      }
    },
    "kdc-bigquery-connector": {
      "projectId": "local-receiver-192608",
      "dataSet": "probes_iot",
      "credentials": {
      },
      "probes": {
        "probe_watcher_1": {
          "type": "watcher",
          "tableName": "sensor",
          "timestamp": "true",
          "schema": {
            "fields": [
              {
                "name": "temperature",
                "type": "FLOAT",
                "mode": "NULLABLE"
              },
              {
                "name": "light_level",
                "type": "FLOAT",
                "mode": "NULLABLE"
              },
              {
                "name": "relative_humidity",
                "type": "FLOAT",
                "mode": "NULLABLE"
              }
            ]
          }
        },
      }
    }
  }
}
